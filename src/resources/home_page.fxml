<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="530.0" prefWidth="899.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HomePage">
    <Pane prefHeight="60.0" prefWidth="899.0">
        <Label alignment="CENTER" layoutX="262.0" layoutY="3.0" prefHeight="32.0"
               prefWidth="375.0" text="Phone Book ">
            <font>
                <Font name="Agency FB Bold" size="45.0"/>
            </font>
        </Label>
    </Pane>
    <AnchorPane layoutX="12.0" layoutY="64.0" prefHeight="459.0" prefWidth="875.0">
        <AnchorPane layoutX="14.0" layoutY="14.0" prefHeight="439.0" prefWidth="274.0">
            <TextField fx:id="mName" layoutX="15.0" layoutY="130.0" prefHeight="25.0"
                       prefWidth="244.0" promptText="username"/>
            <TextField fx:id="mPhone" layoutX="15.0" layoutY="195.0" prefHeight="25.0"
                       prefWidth="244.0" promptText="Phone"/>
            <Button fx:id="btn_save" layoutX="110.0" layoutY="273.0" prefHeight="25.0" prefWidth="55.0"
                    text="Save"/>
