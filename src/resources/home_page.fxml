                    prefWidth="575.0">
            <TableView fx:id="tableView" layoutX="9.0" layoutY="35.0" prefHeight="390.0" prefWidth="558.0">
                <columns>
                    <TableColumn fx:id="column_username" prefWidth="279.199951171875" text="Username"/>
                    <TableColumn fx:id="column_phone" minWidth="0.0" prefWidth="275.20001220703125"
                                 text="Phone Number"/>
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
            </TableView>
            <Button fx:id="btn_delete" layoutX="504.0" layoutY="4.0" prefHeight="25.0" prefWidth="55.0"
                    text="Delete"/>
        </AnchorPane>
    </AnchorPane>
</AnchorPane>
